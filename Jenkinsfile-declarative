pipeline{
    agent any

    environment {
        project = "Test"
    }
    stages {
        stage('build'){
            steps{
                sh "sh untilloop.sh"
            }
        }
        stage('Array'){
            steps{
                sh "sh array"
            }
        }
    }
    post {
       echo "Success"
       sh 'mail -s "Success momire09@gmail.com'
    }
    failure {
        echo "Failed"
	sh 'mail -s "Failed" momire09@gmail.com'
    }	
    always {
        deleteDir()
	}
   	
}    
